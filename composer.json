{
    "name": "carlosre/laravel-preset-tailwindcss-sass",
    "description": "Laravel 5.5.x or 5.6.x preset for Tailwind CSS & Sass",
    "keywords": ["laravel", "preset", "tailwindcss", "sass"],
    "license": "MIT",
    "require": {
        "laravel/framework": "5.5.*|5.6.*"
    },
    "autoload": {
        "psr-4": {
            "CarlosRE\\LaravelPresetTailwindCssSass\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarlosRE\\LaravelPresetTailwindCssSass\\TailwindCssSassPresetServiceProvider"
            ]
        }
    }
}